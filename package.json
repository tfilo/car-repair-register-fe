{
    "name": "car-repair-register-fe",
    "author": {
        "name": "Tomáš Filo (tope-sk)",
        "url": "https://github.com/tope-sk/car-repair-register-fe"
    },
    "license": "MIT",
    "version": "1.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "npm run apigen && tsc -b && vite build",
        "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
        "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
        "prettier": "prettier 'src/**/*.{js,jsx,ts,tsx}' --check",
        "prettier:fix": "prettier 'src/**/*.{js,jsx,ts,tsx}' --write",
        "apigen": "rimraf src/api/openapi && openapi-generator-cli generate",
        "cy:open": "npm run cy:clear && cypress open --e2e --browser electron",
        "cy:run": "npm run cy:clear && cypress run --browser electron",
        "cy:clear": "rimraf cypress/results/*.xml",
        "licenseReport": "license-report --package=./package.json --output=table --config license-report.json > ./LIBRARIES"
    },
    "dependencies": {
        "@mui/icons-material": "^6.2.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@cypress/code-coverage": "^3.13.9",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@eslint/js": "^9.17.0",
        "@fontsource/roboto": "^5.1.0",
        "@mui/base": "^5.0.0-beta.68",
        "@mui/material": "^6.2.1",
        "@mui/x-date-pickers": "^7.23.3",
        "@openapitools/openapi-generator-cli": "^2.15.3",
        "@tanstack/eslint-plugin-query": "^5.62.1",
        "@tanstack/react-form": "^0.40.4",
        "@tanstack/react-query": "^5.62.8",
        "@tanstack/react-query-devtools": "^5.62.8",
        "@tanstack/react-router": "^1.92.3",
        "@tanstack/router-devtools": "^1.92.3",
        "@tanstack/router-plugin": "^1.91.1",
        "@tanstack/yup-form-adapter": "^0.40.4",
        "@types/lodash": "^4.17.13",
        "@types/react": "^18.3.17",
        "@types/react-dom": "^18.3.5",
        "@vitejs/plugin-react": "^4.3.4",
        "cypress": "^13.17.0",
        "eslint": "^9.17.0",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.16",
        "globals": "^15.13.0",
        "mocha-junit-reporter": "^2.2.1",
        "oidc-client-ts": "^3.1.0",
        "prettier": "^3.4.2",
        "react-oidc-context": "^3.2.0",
        "rimraf": "^6.0.1",
        "sass-embedded": "^1.83.0",
        "typescript": "~5.6.2",
        "typescript-eslint": "^8.18.1",
        "use-debounce": "^10.0.4",
        "vite": "^6.0.3",
        "vite-plugin-html": "^3.2.2",
        "vite-plugin-istanbul": "^6.0.2",
        "vite-plugin-minify": "^2.1.0",
        "yup": "^1.6.1",
        "yup-locales": "^1.2.28",
        "license-report": "^6.7.0"
    }
}

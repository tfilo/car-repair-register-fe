{
    "name": "car-repair-register-fe",
    "author": {
        "name": "Tomáš Filo",
        "url": "https://github.com/tfilo/car-repair-register-fe"
    },
    "license": "MIT",
    "version": "1.3.2",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "npm run apigen && tsc -b && vite build",
        "build:dev": "npm run apigen && tsc -b && CYPRESS_COVERAGE=true vite build -m development",
        "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
        "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
        "prettier": "prettier 'src/**/*.{js,jsx,ts,tsx}' --check",
        "prettier:fix": "prettier 'src/**/*.{js,jsx,ts,tsx}' --write",
        "apigen": "rimraf src/api/openapi && openapi-generator-cli generate",
        "cy:open": "npm run cy:clear && cypress open --e2e --browser electron",
        "cy:run": "npm run cy:clear && cypress run --browser electron",
        "cy:clear": "rimraf cypress/results/*.xml",
        "licenseReport": "license-report --package=./package.json --output=table --config license-report.json > ./LIBRARIES"
    },
    "dependencies": {
        "react": "^19.2.0",
        "react-dom": "^19.2.0"
    },
    "devDependencies": {
        "@cypress/code-coverage": "^3.14.7",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@eslint/js": "^9.23.0",
        "@fontsource/roboto": "^5.2.8",
        "@hookform/resolvers": "^4.1.3",
        "@mui/icons-material": "^6.5.0",
        "@mui/material": "^6.5.0",
        "@mui/x-date-pickers": "^7.29.4",
        "@openapitools/openapi-generator-cli": "^2.24.0",
        "@tanstack/eslint-plugin-query": "^5.91.0",
        "@tanstack/react-query": "^5.90.2",
        "@tanstack/react-query-devtools": "^5.90.2",
        "@tanstack/react-router": "^1.132.47",
        "@tanstack/react-router-devtools": "^1.132.51",
        "@tanstack/router-plugin": "^1.132.51",
        "@types/lodash": "^4.17.20",
        "@types/pg": "^8.15.5",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.1",
        "@vitejs/plugin-react": "^4.7.0",
        "cypress": "^14.5.4",
        "date-fns": "^4.1.0",
        "eslint": "^9.37.0",
        "eslint-plugin-react-compiler": "^19.1.0-rc.2",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.23",
        "globals": "^16.4.0",
        "license-report": "^6.8.0",
        "lodash": "^4.17.21",
        "mocha-junit-reporter": "^2.2.1",
        "oidc-client-ts": "^3.3.0",
        "pg": "^8.16.3",
        "prettier": "^3.6.2",
        "react-hook-form": "^7.65.0",
        "react-oidc-context": "^3.3.0",
        "rimraf": "^6.0.1",
        "sass-embedded": "^1.93.2",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.0",
        "use-debounce": "^10.0.6",
        "vite": "^6.3.6",
        "vite-plugin-html": "^3.2.2",
        "vite-plugin-istanbul": "^7.2.0",
        "vite-plugin-minify": "^2.1.0",
        "yup": "^1.7.1",
        "yup-locales": "^1.2.28"
    }
}

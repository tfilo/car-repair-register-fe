{
    "name": "car-repair-register-fe",
    "author": {
        "name": "Tomáš Filo (tope-sk)",
        "url": "https://github.com/tope-sk/car-repair-register-fe"
    },
    "license": "MIT",
    "version": "1.2.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "npm run apigen && tsc -b && vite build",
        "build:dev": "npm run apigen && tsc -b && CYPRESS_COVERAGE=true vite build -m dev",
        "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
        "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
        "prettier": "prettier 'src/**/*.{js,jsx,ts,tsx}' --check",
        "prettier:fix": "prettier 'src/**/*.{js,jsx,ts,tsx}' --write",
        "apigen": "rimraf src/api/openapi && openapi-generator-cli generate",
        "cy:open": "npm run cy:clear && cypress open --e2e --browser electron",
        "cy:run": "npm run cy:clear && cypress run --browser electron",
        "cy:clear": "rimraf cypress/results/*.xml",
        "licenseReport": "license-report --package=./package.json --output=table --config license-report.json > ./LIBRARIES"
    },
    "dependencies": {
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@cypress/code-coverage": "^3.13.12",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@eslint/js": "^9.23.0",
        "@fontsource/roboto": "^5.2.5",
        "@hookform/resolvers": "^4.1.3",
        "@mui/icons-material": "^6.4.8",
        "@mui/material": "^6.4.8",
        "@mui/x-date-pickers": "^7.28.0",
        "@openapitools/openapi-generator-cli": "^2.18.4",
        "@tanstack/eslint-plugin-query": "^5.68.0",
        "@tanstack/react-query": "^5.69.0",
        "@tanstack/react-query-devtools": "^5.69.0",
        "@tanstack/react-router": "^1.114.7",
        "@tanstack/react-router-devtools": "^1.114.27",
        "@tanstack/router-plugin": "^1.114.27",
        "@types/lodash": "^4.17.16",
        "@types/pg": "^8.11.11",
        "@types/react": "^19.0.12",
        "@types/react-dom": "^19.0.4",
        "@vitejs/plugin-react": "^4.3.4",
        "cypress": "^14.2.0",
        "eslint": "^9.23.0",
        "eslint-plugin-react-compiler": "^19.0.0-beta-3229e95-20250315",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^16.0.0",
        "license-report": "^6.7.2",
        "lodash": "^4.17.21",
        "mocha-junit-reporter": "^2.2.1",
        "oidc-client-ts": "^3.2.0",
        "pg": "^8.14.1",
        "prettier": "^3.5.3",
        "react-hook-form": "^7.54.2",
        "react-oidc-context": "^3.2.0",
        "rimraf": "^6.0.1",
        "sass-embedded": "^1.86.0",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.27.0",
        "use-debounce": "^10.0.4",
        "vite": "^6.2.2",
        "vite-plugin-html": "^3.2.2",
        "vite-plugin-istanbul": "^7.0.0",
        "vite-plugin-minify": "^2.1.0",
        "yup": "^1.6.1",
        "yup-locales": "^1.2.28"
    }
}
